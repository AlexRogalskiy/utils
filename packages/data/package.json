{
    "name": "@alfalab/data",
    "version": "1.1.0",
    "description": "common data",
    "sideEffects": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/esm/index.js",
    "files": [
        "dist",
        "*.js",
        "*.mjs",
        "*.d.ts",
        "!rollup.config.js"
    ],
    "scripts": {
        "update:exports": "node ../../scripts/update-exports.js",
        "build": "yarn build:ts && yarn update:exports && yarn build:dts ",
        "build:ts": "rollup -c",
        "build:dts": "dts-bundle-generator --no-banner --config dts.json"
    },
    "keywords": [],
    "author": "Evgeniy Troynov",
    "license": "MIT",
    "devDependencies": {
        "dts-bundle-generator": "^5.6.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "gitHead": "10dd0711fab88c3173da9cd72abe50194559d021",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./package.json": "./package.json",
        "./countries": {
            "import": "./dist/countries/index.mjs",
            "require": "./dist/countries/index.js"
        },
        "./currency": {
            "import": "./dist/currency/index.mjs",
            "require": "./dist/currency/index.js"
        },
        "./keyboard-codes": {
            "import": "./dist/keyboard-codes/index.mjs",
            "require": "./dist/keyboard-codes/index.js"
        }
    }
}
